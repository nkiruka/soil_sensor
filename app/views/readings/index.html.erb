<table id="reading" class="display table table-bordered" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th>Sensor</th>
      <th>Reading</th>
      <th>Time Stamp</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @readings.each do |reading| %>
      <tr>
        <td><%= reading.sensor_id %></td>
        <td><%= reading.value %></td>
        <td><%= reading.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
